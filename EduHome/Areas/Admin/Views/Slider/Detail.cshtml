@model Slider
<div class="row">
    <img src="~/img/slider/@Model.Image" width="400px" height="400px"/>
</div>
<div>
    <h3 class="mt-3">Title</h3>
    @Model.Title
</div>
<div>
    <h3 class="mt-3">Description</h3>
    @Model.Description
</div>
<div class="row mt-3">
    <a class="btn btn-dark" asp-action="Index">Go Back</a>
</div>
